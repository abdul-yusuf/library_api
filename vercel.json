{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ],
    "env": {
      "MONGODB_URI": "mongodb+srv://<man>:<password>@cluster.mongodb.net/library?retryWrites=true&w=majority",
      "PORT": "3000"
    }
  }
  